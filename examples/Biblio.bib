	author = {Talreja, Ramesh and Veer Singh, Chandra},
	year = {2012}
}

@article{wagner_significance_1991,
	title = {The significance of microcomposites as experimental models},
	volume = {12},
	issn = {1548-0569},
	url = {http://onlinelibrary.wiley.com/doi/10.1002/pc.750120404/abstract},
	doi = {10.1002/pc.750120404},
	abstract = {This paper demonstrates the usefulness of careful experimental work with model composite materials, such as thin polymeric films in which single fibers are accurately positioned, in at least two respects: to assess the validity of a theory for a given physical property, and to accurately probe the effects of various parameters on the behavior of composites. Working with such model composites has obvious advantages, such as the full control of experimental parameters, the possibility of introducing perturbative effects in a controlled way, and the possibility of verifying theoretical models in the range of low fiber content. Indeed, macroscopic composite materials contain various types of defects and perturbative effects, such as fiber misalignment or slack, fiber-poor regions, voids, etc., which bias any quantitative assessment of mechanical and physical properties, and preclude the accurate verification of theoretical schemes. One difficulty in working with microcomposite models, also recalled here, is the need for an appropriate “scaling-up” procedure to the level of macroscopic composites.},
	language = {en},
	number = {4},
	urldate = {2017-03-06},
	journal = {Polymer Composites},
	author = {Wagner, H. D. and Rubins, M. and Marom, G.},
	month = aug,
