@article{guo_properties_2014,
	title = {Properties of {Polylactide} {Inks} for {Solvent}-{Cast} {Printing} of {Three}-{Dimensional} {Freeform} {Microstructures}},
	volume = {30},
	issn = {0743-7463},
	url = {http://dx.doi.org/10.1021/la4036425},
	doi = {10.1021/la4036425},
	abstract = {Solvent-cast printing is a highly versatile microfabrication technique that can be used to construct various geometries such as filaments, towers, scaffolds, and freeform circular spirals by the robotic deposition of a polymer solution ink onto a moving stage. In this work, we have performed a comprehensive characterization of the solvent-cast printing process using polylactide (PLA) solutions by analyzing the flow behavior of the solutions, the solvent evaporation kinetics, and the effect of process-related parameters on the crystallization of the extruded filaments. Rotational rheometry at low to moderate shear rates showed a nearly Newtonian behavior of the PLA solutions, while capillary flow analysis based on process-related data indicated shear thinning at high shear rates. Solvent vaporization tests suggested that the internal diffusion of the solvent through the filaments controlled the solvent removal of the extrudates. Different kinds of three-dimensional (3D) structures including a layer-by-layer tower, nine-layer scaffold, and freeform spiral were fabricated, and a processing map was given to show the proper ranges of process-related parameters (i.e., polymer content, applied pressure, nozzle diameter, and robot velocity) for the different geometries. The results of differential scanning calorimetry revealed that slow solvent evaporation could increase the ability of PLA to complete its crystallization process during the filament drying stage. The method developed here offers a new perspective for manufacturing complex structures from polymer solutions and provides guidelines to optimize the various parameters for 3D geometry fabrication.},
	number = {4},
	urldate = {2015-11-23},
	journal = {Langmuir},
	author = {Guo, Shuang-Zhuang and Heuzey, Marie-Claude and Therriault, Daniel},
	month = feb,
	year = {2014},
	pages = {1142--1150},
	file = {ACS Full Text PDF w/ Links:/home/ilyass/Zotero/storage/5QKI2ZF6/Guo et al. - 2014 - Properties of Polylactide Inks for Solvent-Cast Pr.pdf:application/pdf;ACS Full Text Snapshot:/home/ilyass/Zotero/storage/GPAAP5NP/la4036425.html:text/html}
}

