@article{swolfs_synchrotron_2015,
	title = {Synchrotron radiation computed tomography for experimental validation of a tensile strength model for unidirectional fibre-reinforced composites},
	volume = {77},
	issn = {1359-835X},
	url = {http://www.sciencedirect.com/science/article/pii/S1359835X15002171},
	doi = {10.1016/j.compositesa.2015.06.018},
	abstract = {Synchrotron radiation computed tomography has been used to analyse fibre break accumulation in unidirectional composites loaded in tension. The data are compared to model predictions. The model only slightly overestimated the composite failure strain, but predictions of fibre break density were too high, which can be mainly attributed to errors in the Weibull distribution. Both the number and percentage of interacting fibre break clusters were under-predicted by the model. This was attributed to an underestimation of stress concentrations in the model. While the experimental observations revealed mainly co-planar clusters, the model predicted mainly diffuse clusters. The experiments showed that the clusters did grow any further after their formation, while the model predicted a gradual development. Both local and dynamic stress concentrations were hypothesised to be key features for further exploration. The discrepancies identified, inform suggestions for directions advancing the state-of-the-art strength models of UD composites.},
	urldate = {2015-08-26},
	journal = {Composites Part A: Applied Science and Manufacturing},
	author = {Swolfs, Y. and Morton, H. and Scott, A. E. and Gorbatikh, L. and Reed, P. A. S. and Sinclair, I. and Spearing, S. M. and Verpoest, I.},
