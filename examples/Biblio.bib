@article{karger-kocsis_microstructure-related_1993,
	series = {Special {Issue} {Microphenomena} in {Advanced} {Composites}},
	title = {Microstructure-related fracture toughness and fatigue crack growth behaviour in toughened, anhydride-cured epoxy resins},
	volume = {48},
