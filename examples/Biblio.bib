	keywords = {Constitutive behavior, Fiber-reinforced composite material, Nonlocal damage, Residual stress, Thermomechanical process},
	pages = {1241--1264},
	file = {ScienceDirect Full Text PDF:/home/ilyass/Zotero/storage/MTNAHJ5K/Heinrich et al. - 2013 - The role of curing stresses in subsequent response.pdf:application/pdf;ScienceDirect Snapshot:/home/ilyass/Zotero/storage/AIGH3S9A/S0022509612002591.html:text/html}
}

