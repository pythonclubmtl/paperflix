	keywords = {A. Polymer–matrix composites (PMCs), B. Residual/internal stress, A. Thermoplastic resin},
	pages = {1847--1857},
	file = {ScienceDirect Full Text PDF:/home/ilyass/Zotero/storage/P6R2VHXW/Parlevliet et al. - 2006 - Residual stresses in thermoplastic composites—A st.pdf:application/pdf;ScienceDirect Snapshot:/home/ilyass/Zotero/storage/H79RKGPH/S1359835X06000145.html:text/html}
}

