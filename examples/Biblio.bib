@article{varna_transverse_1997,
	title = {Transverse single-fibre test for interfacial debonding in composites: 2. {Modelling}},
	volume = {28},
	issn = {1359-835X},
	shorttitle = {Transverse single-fibre test for interfacial debonding in composites},
	url = {http://www.sciencedirect.com/science/article/pii/S1359835X9600125X},
	doi = {10.1016/S1359-835X(96)00125-X},
	abstract = {A method was developed for determination of interfacial toughness parameters from a new transverse single-fibre test. In previous work, experimental procedures were developed. Interfacial debond growth in the arc and fibre direction was measured for transparent matrix polymer composites. In the present study, an earlier stress analysis by Toya was modified and combined with a critical energy release rate criterion, taking mode mixity effects into account. The developed model was used to determine interfacial toughness parameters. The model was able to describe and explain the main experimental observations of the debonding process. Treatment of the contact problem present for large debond angles is of interest for future work.},
	number = {4},
	urldate = {2018-09-17},
	journal = {Composites Part A: Applied Science and Manufacturing},
	author = {Varna, J. and Berglund, L. A. and Ericson, M. L.},
	month = jan,
	year = {1997},
	keywords = {interfacial debonding, fibre-matrix interface, modelling, transverse single-fibre test},
	pages = {317--326}
}

